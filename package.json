{
  "name": "dj-tour",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:fast": "next dev --turbo",
    "dev:port": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules/.cache && npm install",
    "kill-ports": "lsof -ti:3000,3001,3002,3003 | xargs kill -9 2>/dev/null || true",
    "dev:clean": "npm run clean && npm run kill-ports && npm run dev",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@stripe/stripe-js": "^7.4.0",
    "@supabase/supabase-js": "^2.39.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vercel/analytics": "^1.5.0",
    "autoprefixer": "^10.0.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dotenv": "^17.2.0",
    "framer-motion": "^12.23.3",
    "lucide-react": "^0.263.1",
    "next": "14.1.0",
    "openai": "^5.9.0",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "stripe": "^18.3.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.0"
  },
  "description": "Un site web pour les DJs qui veulent développer leur carrière avec 100 missions gamifiées. Booste tes bookings, ta visibilité et tes revenus étape par étape.",
  "main": "postcss.config.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
